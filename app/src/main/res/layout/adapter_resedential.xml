<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="165dp"
    android:layout_height="220dp"
    android:layout_margin="13dp"
    xmlns:app1="http://schemas.android.com/apk/res-auto">



    <ImageView
        android:id="@+id/cat_img"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="fitXY"
        android:background="@drawable/apartment"
        app1:layout_constraintTop_toTopOf="parent"
        app1:layout_constraintLeft_toLeftOf="parent"
        app1:layout_constraintRight_toRightOf="parent"

        />

    <TextView
        android:id="@+id/property_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="5dp"
        app1:layout_constraintLeft_toLeftOf="parent"
        app1:layout_constraintTop_toTopOf="parent"
        app1:layout_constraintRight_toRightOf="parent"
        android:fontFamily="sans-serif-medium"
        android:layout_margin="20dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17sp">
    </TextView>

    <TextView
        android:id="@+id/propertycount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        app1:layout_constraintTop_toBottomOf="@+id/property_name"
        app1:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp">
    </TextView>




</androidx.constraintlayout.widget.ConstraintLayout>